@{
    ViewData["Title"] = "Home Page";
}
@foreach(Foro f in ViewBag.ListarForos)
{
    <a class="cardc" href= '@Url.Action("Foro", new {idForo = f.IdForo})'>
        <div class= "CardCategoria"> 
            <h3>@f.Titulo </h3>
            <div><img src= ""></div>
            <div><p>@f.Descripcion </p></div>
        </div>
    </a>
}